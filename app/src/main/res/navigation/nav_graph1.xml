<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph1"
    app:startDestination="@id/blank_fragment">

    <fragment
        android:id="@+id/blank_fragment"
        android:name="com.example.navigationarchtest.BlankFragment"
        android:label="Blank"
        tools:layout="@layout/fragment_blank">

        <action
            android:id="@+id/action_blankFragment_to_secondFragment"
            app:destination="@id/secondFragment"
            />

    </fragment>

    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.navigationarchtest.SecondFragment"
        android:label="SecondFragment" />

</navigation>